<!DOCTYPE html>
<html>
  <head>
    <% include ../static/partials/head.ejs %>
  </head>
  <body>
    <% include ../static/partials/navbar.ejs %>

    <main class="container">

      <h1>Remove Collaborator</h1>

      <form action="/wikis/<%= wiki.id %>/collaborations/createCollaborationRemove" method="post">
        <div class="form-group">
          <label for="title">User Name</label>

          <input value="<%= wiki.collaboration %>" type="text" class="form-control" name="updateCollaboratorRemove" aria-describedby="titleHelp" placeholder="Collaborator to remove (email)">
        </div>

        <button type="submit" class="btn btn-primary">Remove</button>
      </form>
    </main>

  </body>
</html>
